{
  "name": "Appledaily By-pass",
  "version": "1.0",
  "description": "Browsing AppleDaily without subscription",
  "content_scripts":[{
    "matches": ["*://*.appledaily.com/*"],
    "js": ["content.js"],
    "run_at": "document_start"
  }],
  "permissions": [
    "*://*.appledaily.com/"
  ],
  "manifest_version": 2
}